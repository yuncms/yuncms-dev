{
    "name": "yuncms/yuncms-dev",
    "description": "YUNCMS - Development Package",
    "keywords": ["yuncms", "yii2", "framework", "project template"],
    "homepage": "http://www.yuncms.net/",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yuncms/yuncms-dev/issues?state=open",
        "source": "https://github.com/yuncms/yuncms-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "yuncms/yuncms-composer": "~2.0.0",
        "ezyang/htmlpurifier": "~4.6",
        "cebe/markdown": "~1.0.0 | ~1.1.0",
        "bower-asset/jquery": "3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/inputmask": "~3.2.2 | ~3.3.5",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "cebe/indent": "~1.0.2",
        "friendsofphp/php-cs-fixer": "~2.2.3"
    },
    "suggest": {
        "yiisoft/yii2-coding-standards": "you can use this package to check for code style issues when contributing to yii"
    },
    "config": {
        "platform": {"php": "5.4"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ]
}